<article class="listing">
  <a {{action 'toggleImageSize'}} class="image {{if isWide "wide"}}">
    <img src="{{contract.image}}" alt="">
    <small>Agrandar Imagen</small>
  </a>
  <h3>{{contract.name}}</h3>
  <div class="detail type">
    <span>Tipo:</span> {{contract.type}} \n {{contract-spy-type contract.type}}
  </div>
  <div class="detail city">
    <span>Ciudad:</span> {{contract.city}}
  </div>
</article>

{{yield}}
